# Configuration template
# Copy this file to .env and fill in your values

# Recipient wallet address (where tokens will be sent)
RECIPIENT_ADDRESS=0xYourRecipientAddressHere

# Recipient private key (for auto-refuel feature)
# Leave empty to disable auto-refuel
RECIPIENT_PRIVATE_KEY=

# File containing donor private keys (one per line)
DONOR_FILE=DONOR.txt

# Minimum balance to process (in USD)
MIN_BALANCE_USD=0.05

# Auto-refuel settings
AUTO_REFUEL_GAS=True
MIN_GAS_REFUEL_AMOUNT=0.002

# Threading settings
MAX_PARALLEL_WALLETS=25
PAUSE_BETWEEN_WALLETS=7

# Gas settings
GAS_PRICE_MULTIPLIER=1.2
